<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::content">
                    <resource id="Pre_News::news_content" title="News" sortOrder="10" >
                        <resource id="Pre_News::feed" title="News Feeds" sortOrder="40">
                            <resource id="Pre_News::save" title="Save News Feed" sortOrder="10" />
                            <resource id="Pre_News::feed_delete" title="Delete News Feed" sortOrder="20" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
